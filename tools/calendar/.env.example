# Copy this file to .env and fill in your values.
# Never commit .env to git.

# Secret token that the Referee must enter to save state changes.
# Choose something hard to guess (e.g. a long random string).
REFEREE_TOKEN=zmień-na-tajne-hasło

# Port the server listens on (default: 3000)
# On Mikrus/Cytrus: nginx proxies external port 20131 → localhost:3000
PORT=3000

# CORS: which origin is allowed to call the API
ALLOWED_ORIGIN=https://edrache.cytr.us
